---
import Welcome from '../components/Welcome.astro';
import Layout from '../layouts/Layout.astro';
import TestNote from './test.md';  // optional, if you want to embed Markdown
---

<Layout>
  <!-- Keep the Welcome component -->
  <Welcome />

  <!-- Your own content below -->
  <h2>My Latest Notes</h2>
  <p>Here is a test note I added to the homepage.</p>

  <!-- Example: embed a Markdown note -->
  <TestNote />

  <!-- Or just link to your notes -->
  <ul>
    <li><a href="/test">Test Note</a></li>
    <li><a href="/another-note">Another Note</a></li>
  </ul>
</Layout>